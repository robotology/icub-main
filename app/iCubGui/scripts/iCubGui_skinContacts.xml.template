<application>
 <name>iCubGui with Skin Contacts</name>
   <module>
    <name>iCubGui</name>
    <parameters>--xpos 1000 --ypos 100 --width 370</parameters> 
    <node>console-node</node>
    <tag>iCubGui</tag>
  </module>

  <connection>
    <from>/skinManager/skin_events:o</from>
    <to>/iCubGui/forces</to>
    <protocol>udp</protocol>
  </connection>
  <connection>
    <from>/icub/head/state:o</from>
    <to>/iCubGui/head:i</to>
    <protocol>udp</protocol>
  </connection>
  <connection>
    <from>/icub/head/inertials/measures:o</from>
    <to>/iCubGui/inertials/measures:i</to>
    <protocol>udp</protocol>
  </connection>
  <connection>
    <from>/icub/left_arm/state:o</from>
    <to>/iCubGui/left_arm:i</to>
    <protocol>udp</protocol>
  </connection>
  <connection>
    <from>/icub/right_arm/state:o</from>
    <to>/iCubGui/right_arm:i</to>
    <protocol>udp</protocol>
  </connection>
   <connection>
    <from>/icub/left_leg/state:o</from>
    <to>/iCubGui/left_leg:i</to>
    <protocol>udp</protocol>
  </connection>
  <connection>
    <from>/icub/right_leg/state:o</from>
    <to>/iCubGui/right_leg:i</to>
    <protocol>udp</protocol>
  </connection>
  <connection>
    <from>/icub/torso/state:o</from>
    <to>/iCubGui/torso:i</to>
    <protocol>udp</protocol>
  </connection>
</application>
