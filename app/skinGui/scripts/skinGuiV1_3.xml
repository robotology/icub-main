  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/icub/skin/leftforearm_uppercover</from>
  <to>/skinGui/left_forearm_upper:i</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/icub/skin/leftforearm_lowercover</from>
  <to>/skinGui/left_forearm_lower:i</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/icub/skin/rightforearm_uppercover</from>
  <to>/skinGui/right_forearm_upper:i</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/icub/skin/rightforearm_lowercover</from>
  <to>/skinGui/right_forearm_lower:i</to>
