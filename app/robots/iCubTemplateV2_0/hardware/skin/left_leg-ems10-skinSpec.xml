<!DOCTYPE params PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<params robot="iCubDarmstadt01" build="1">

        <group name="defaultCfgBoard">
            <param name="period">        50  </param> 
            <param name="skinType">      0  </param> <!-- 0 ==> new skin
                                                              1 ==> palm and fingertips skin
                                                              2 ==> old skin without temperature compensation -->
            <param name="noLoad">         0xf0  </param>
            <param name="diagnostic">    false  </param>     <!-- used to config high level -->
        </group>
        
        
       <group name="defaultCfgTriangle">
            <param name="enabled">         false  </param> 
            <param name="shift">           2  </param>
            <param name="cdcOffset">       0x2200  </param>
        </group>
		
		<group name="specialCfgTriangles">
        <param name="numOfSets">         17  </param> <!-- id starts from 1 for triangleSetCfg-->
        <!--                                 patch       boardAddr    idStart       idEnd        enabled          shift       cdcOffset      -->
        
			<!-- UPPER LEG -->
			
		   <param name="triangleSetCfg1">      0           1            0            15             1              2            0x2200      </param>
		   
		   <param name="triangleSetCfg2">      0            2            0             0             1              2            0x2200      </param>
		   <param name="triangleSetCfg3">      0            2            3             6             1              2            0x2200      </param>
		   <param name="triangleSetCfg4">      0            2            9            11             1              2            0x2200      </param>
		   <param name="triangleSetCfg5">      0           2           14            15             1              2            0x2200      </param>
		   
		   <param name="triangleSetCfg6">      0            3            1             8             1              2            0x2200      </param>
		   <param name="triangleSetCfg7">      0            3           12            13             1              2            0x2200      </param>
		  
		   <param name="triangleSetCfg8">      0            4            5             6             1              2            0x2200      </param>
		   <param name="triangleSetCfg9">      0            4            8            10             1              2            0x2200      </param>
		   
		   <param name="triangleSetCfg10">      0            5            0             2             1              2            0x2200      </param>
		   <param name="triangleSetCfg11">      0            5           12            12             1              2            0x2200      </param>
		   <param name="triangleSetCfg12">      0            5           14            15             1              2            0x2200      </param>
		   
		   <param name="triangleSetCfg13">      0            6           1            1              1              2            0x2200       </param>
		   <param name="triangleSetCfg14">      0            6           3            13             1              2            0x2200       </param>
		   
		   <param name="triangleSetCfg15">       0            7           0            1              1              2            0x2200       </param>
		   <param name="triangleSetCfg16">      0            7           8            8              1              2            0x2200       </param>
		   <param name="triangleSetCfg17">      0            7          10           15              1              2            0x2200       </param>
		  
		
        </group>
        
</params>
