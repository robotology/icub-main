        <param name="Inertia">0.00000847</param> <!-- Motor Inertia [Kg m^2] -->
        <param name="NumMagnPoles">8</param> <!-- Number of magnetic poles of the motor-->
        <param name="MaxStaticFriction">0.015</param> <!-- Max static friction [Nm] -->
        <param name="ViscousDamping">0.00001995802986</param> <!-- * Viscous damping [Nm / rad/s] -->
        <param name="PeakTorque">0.806</param> <!-- Peak torque [Nm] -->
        <param name="StallTorque">0.223</param> <!-- Stall torque [Nm] -->
        <param name="ReductionFactor">100</param> <!-- * Transmission reduction -->
        <param name="MaxVoltage">48</param> <!-- * Maximum voltage [V] -->
