<!DOCTYPE devices PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<devices robot="iCubGenova01" build="1">
  <device name="torso_mspecs" type="motorspecifications">   
      
      <group name="DEVICE">
          <param name="rel_candevice">torso_mc</param> <!-- This is the name of the device in which there are the encoder, joints and pids -->
      </group>
      
<group name="MOTORS">
