        <param name="MaxVoltage">48</param> <!-- * Maximum voltage [V] -->
    </group>
</group>

<group name="COUPLED_JOINTS">
    <group name="Roll_Pitch">
        <param name="JointNumber">2</param>
        <param name="JointIndexes">0 1</param>
        <param name="VelocityTransfMatrixCoefficient">1</param>
        <param name="VelocityTransfMatrix">((0.5 0.5) (0.5 -0.5))</param> <!-- Matrix transforming from encoder axis/coordinates to joint coordinates -->
        <param name="TorqueTransfMatrixCoefficient">1</param>
        <param name="TorqueTransfMatrix">((1 1) (1 -1))</param> <!-- Matrix transforming from joint coordinates to motor coordinates -->
    </group>
</group>
  
  </device>    
</devices>     
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
