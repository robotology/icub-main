    <group name="Motor1">
        <param name="Resistance">0.664</param> <!-- * Motor Resistance [Ohm] -->
        <param name="Inductance">0.32</param> <!-- Motor Inductance [mH] -->
        <param name="BackEMF">0.04096648235185</param> <!-- * Motor Back ElectroMagnetic Force Constant [V/ rad/s] -->
