<!-- instantiation of
     $ICUB_ROOT/main/app/iCubStartup/scripts/iCubStartup.xml.template
     for iCubLisboa01 -->

<application>
<name>iCubStartup for iCubLisboa01 (Chico)</name>

        <dependencies>
        </dependencies>
      
        <module>
                <name>iCubInterface</name>
                <node>pc104</node>
                <stdio>chico3</stdio>
                <tag>icubInterface</tag>
        </module>

	<!--		
	<module>
                <name>iCubInterfaceGuiClient</name>
                <node>chico3</node>
                <stdio>chico3</stdio>
                <tag>iCubInterfaceGuiClient</tag>
        </module>
	-->
   
        <module>
                <name>iKinCartesianSolver</name>
                <parameters>--part right_arm</parameters>
                <node>icubbrain1</node>
                <tag>rightArmSolver</tag>
        </module>

        <module>
                <name>iKinCartesianSolver</name>
                <parameters>--part left_arm</parameters>
                <node>icubbrain2</node>
                <tag>leftArmSolver</tag>
        </module>

	<!--
        <module>
                <name>iKinCartesianSolver</name>
                <parameters>- -part right_leg</parameters>
                <node>pwrNode3</node>
                <tag>rightLegSolver</tag>
        </module>

        <module>
                <name>iKinCartesianSolver</name>
                <parameters>- -part left_leg</parameters>
                <node>pwrNode4</node>
                <tag>leftLegSolver</tag>
        </module>
	-->

        <module>
                <name>iKinGazeCtrl</name>
		<parameters>--context iCubLisboa01/conf --config icubEyes320x240.ini --eyeTiltMin -7.5 --eyeTiltMax 15.0 --ping_robot_tmo 40.0</parameters>
		<!--
                <parameters>- -context cameraCalibration/conf - -config icubEyes.ini - -eyeTiltMin -7.5 - -eyeTiltMax 15.0 - -ping_robot_tmo 40.0</parameters>
		-->
                <node>icubbrain1</node>
                <tag>gazeController</tag>
        </module>

        <module>
                <name>wholeBodyDynamics</name>
                <parameters>--headV2 --autoconnect</parameters>
                <node>icubbrain1</node>
                <stdio>chico3</stdio>
                <tag>wholeBodyDynamicsTag</tag>
        </module>

        <module>
                <name>gravityCompensator</name>
                <parameters>--headV2</parameters>
                <node>icubbrain2</node>
                <stdio>chico3</stdio>
                <tag>gravityCompensatorTag</tag>
        </module>

	<!--	
        <connection>
                <from>/icubinterfacegui/client</from>
                <to>/icubinterfacegui/server</to>
                <protocol>tcp</protocol>
        </connection>
	-->

</application>
